{
  "PrimaryKeyFile": "",
  "BigcommerceApp": {
    "SuppressLogLevels": [ "DEBUG" ]
  },
  "NginxAccess": {
    "SuppressStatusCodes": [ 200, 204, 302, 304 ]
  },
  "Process": {
    "SuppressNames": [
      "/USR/SBIN/CRON",
      "/etc/mysql/debian-start",
      "/usr/sbin/cron",
      "acpid",
      "crontab",
      "dhclient",
      "dnsmasq",
      "kernel",
      "manage_ips",
      "php-fpm",
      "postfix/cleanup",
      "postfix/discard",
      "postfix/master",
      "postfix/pickup",
      "postfix/postfix-script",
      "postfix/qmgr",
      "postfix/smtp",
      "rsyslogd",
      "terminatord"
    ]
  },
  "Php": {
    "SuppressStackTraces": true,
    "SuppressContentRegexes": [
      "^Failed to write to Twig cache.*$",
      "^Undefined index: MBALoginToken$"
    ]
  },
  "Generic": {
    "SuppressNames": [
      "kernel",
      "php"
    ]
  }
}
